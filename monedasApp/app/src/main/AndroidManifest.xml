<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.salmantino.monedasapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Monedas">
        <activity
            android:name="com.salmantino.monedasapp.monedas.MonedasActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Monedas.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.salmantino.monedasapp.monedaDetail.MonedaDetailActivity"
            android:label="@string/title_activity_moneda_detail"
            android:theme="@style/Theme.Monedas.NoActionBar" />
        <activity
            android:name="com.salmantino.monedasapp.addeditmoneda.AddEditMonedaActivity"
            android:label="@string/title_activity_add_edit"
            android:theme="@style/Theme.Monedas.NoActionBar" />
    </application>

</manifest>
